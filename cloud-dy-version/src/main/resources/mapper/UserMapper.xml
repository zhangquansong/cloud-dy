<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.dy.user.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cloud.dy.version.entity.User">
        <id column="id" property="id"/>
        <result column="user_login_name" property="userLoginName"/>
        <result column="user_name" property="userName"/>
        <result column="user_phone" property="userPhone"/>
        <result column="user_sex" property="userSex"/>
        <result column="user_id_card" property="userIdCard"/>
        <result column="user_face" property="userFace"/>
        <result column="user_code" property="userCode"/>
        <result column="user_type" property="userType"/>
        <result column="user_password" property="userPassword"/>
        <result column="user_asset" property="userAsset"/>
        <result column="user_address" property="userAddress"/>
        <result column="user_longin_last_time" property="userLonginLastTime"/>
        <result column="user_status" property="userStatus"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="user_version" property="userVersion"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_login_name AS userLoginName, user_name AS userName, user_phone AS userPhone, user_sex AS userSex,
        user_id_card AS userIdCard, user_face AS userFace, user_code AS userCode, user_type AS userType, user_password
        AS userPassword, user_asset AS userAsset, user_address AS userAddress, user_longin_last_time AS
        userLonginLastTime, user_status AS userStatus, create_time AS createTime, update_time AS updateTime,
        user_version AS userVersion
    </sql>

</mapper>
